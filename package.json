{
  "name": "fore-template",
  "version": "2.0.0",
  "description": "A free responsive site template",
  "main": "index.html",
  "scripts": {
    "less": "lessc less/fore.less css/fore.css",
    "watch": "less-watch-compiler less css fore.less",
    "minify-css": "cleancss -o css/fore.min.css css/fore.css",
    "minify-js": "uglifyjs js/jquery.event.move.js js/jquery.event.swipe.js js/unslider.js -o js/fore.min.js",
    "build": "npm run less && npm run minify-css && npm run minify-js",
    "start": "serve ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/fore-template.git"
  },
  "keywords": [
    "template",
    "responsive",
    "css",
    "html",
    "javascript"
  ],
  "author": "Original by Jordan Bowman",
  "license": "CC-BY-3.0",
  "bugs": {
    "url": "https://github.com/yourusername/fore-template/issues"
  },
  "homepage": "https://github.com/yourusername/fore-template",
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "less": "^4.2.0",
    "less-watch-compiler": "^1.16.3",
    "serve": "^14.2.1",
    "uglify-js": "^3.17.4"
  }
}
